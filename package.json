    "url": "https://github.com/basham/webpack-lab/issues"
  },
  "homepage": "https://github.com/basham/webpack-lab#readme",
  "devDependencies": {
    "@cycle/core": "^1.0.1",
    "@cycle/dom": "^3.1.1",
    "babel-core": "^5.8.19",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.15.6",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "raw-loader": "^0.5.1",
    "react": "^0.13.3",
